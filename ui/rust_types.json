    "AskPairAB_for_CardId": {
      "type": "object",
      "required": [
        "a",
        "b"
      ],
      "properties": {
        "a": {
          "type": "object",
          "additionalProperties": {
            "type": "array",
            "items": [
              {
                "type": "integer",
                "format": "uint",
                "minimum": 0.0
