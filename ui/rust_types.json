        "a",
        "b"
      ],
      "properties": {
        "a": {
          "type": "object",
          "additionalProperties": {
            "type": "array",
            "items": [
              {
                "type": "integer",
                "format": "uint",
                "minimum": 0.0
              },
              {
                "type": "integer",
                "format": "uint",
                "minimum": 0.0
              }
            ],
            "maxItems": 2,
            "minItems": 2
          }
        },
        "b": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/TargetId"
          },
          "uniqueItems": true
        }
      }
    },
    "AskSelectN_for_Action": {
